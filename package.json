    "clean": "rm -rf .turbo && rm -rf dist"
  },
  "type": "module",
  "author": "Glitch-txs",
  "license": "MIT",
  "dependencies": {
  },
  "peerDependencies": {
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "keywords": [
    "contracts",
    "ronin",
    "defi",
    "network",
    "web3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/roninbuilders/contracts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^6.7.0",
    "@biomejs/biome": "1.3.3"
  }
}
